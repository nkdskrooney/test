package com.internousdev.listening.dto;

public class ItemDTO {
	private int itemId;
	private String itemName;
	private String item_name_kana;
	private int category_id;
	private int price;
	private int stock;
	private String image_file_path;
	private String release_date;
	private String regist_date;
	private String update_date;

	public int getItemId() {
		return itemId;
	}
	public String getItemName() {
		return itemName;
	}
	public String getItem_name_kana() {
		return item_name_kana;
	}
	public int getCategory_id() {
		return category_id;
	}
	public int getPrice() {
		return price;
	}
	public int getStock() {
		return stock;
	}
	public String getImage_file_path() {
		return image_file_path;
	}
	public String getRelease_date() {
		return release_date;
	}
	public String getRegist_date() {
		return regist_date;
	}
	public String getUpdate_date() {
		return update_date;
	}
	public void setItemId(int itemId) {
		this.itemId = itemId;
	}
	public void setItemName(String itemName) {
		this.itemName = itemName;
	}
	public void setItem_name_kana(String item_name_kana) {
		this.item_name_kana = item_name_kana;
	}
	public void setCategory_id(int category_id) {
		this.category_id = category_id;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public void setStock(int stock) {
		this.stock = stock;
	}
	public void setImage_file_path(String image_file_path) {
		this.image_file_path = image_file_path;
	}
	public void setRelease_date(String release_date) {
		this.release_date = release_date;
	}
	public void setRegist_date(String regist_date) {
		this.regist_date = regist_date;
	}
	public void setUpdate_date(String update_date) {
		this.update_date = update_date;
	}

}
